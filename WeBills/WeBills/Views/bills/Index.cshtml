@model IEnumerable<WeBills.Models.bills>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.homeid)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.waterCost)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.electricCost)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.homeid)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.waterCost)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.electricCost)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.billid }) |
            @Html.ActionLink("Details", "Details", new { id=item.billid }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.billid })
        </td>
    </tr>
}

</table>
